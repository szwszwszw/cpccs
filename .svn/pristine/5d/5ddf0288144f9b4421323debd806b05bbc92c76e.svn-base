package com.wechat_small_program.entity;

/**
 * 对应数据库中的“答题比赛情况（t_match_list）”表，用于hibernate中ORM映射
 * @author SZW
 * @date 2018年5月22日
 * @version 1.0 
 */
public class MatchEntity {
	private int id;
	private String uid;
	private String grade;
	private String time;
	private int matchId;
	public MatchEntity() {
		super();
		// TODO Auto-generated constructor stub
	}
	public MatchEntity(int id, String uid, String grade, String time,
			int matchId) {
		super();
		this.id = id;
		this.uid = uid;
		this.grade = grade;
		this.time = time;
		this.matchId = matchId;
	}
	@Override
	public String toString() {
		return "MatchEntity [id=" + id + ", uid=" + uid + ", grade=" + grade
				+ ", time=" + time + ", matchId=" + matchId + "]";
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getUid() {
		return uid;
	}
	public void setUid(String uid) {
		this.uid = uid;
	}
	public String getGrade() {
		return grade;
	}
	public void setGrade(String grade) {
		this.grade = grade;
	}
	public String getTime() {
		return time;
	}
	public void setTime(String time) {
		this.time = time;
	}
	public int getMatchId() {
		return matchId;
	}
	public void setMatchId(int matchId) {
		this.matchId = matchId;
	}
	
}
