body{
	display: block;
	margin: 0;
	padding: 0;
	border: 0;
}
.manage_info{
	position: absolute;
	height: 20%;
	width: 7%;
	background-color: rgba(245,168,84,0.1);
	top:7%;
	left: 80%;
	border-radius:10%;
	padding-right: 1%;
}
iframe{
	border-width: 0;
}
.global_variable{
	position: absolute;
	height: 100%;
	width: 100%;
	background-color: #272821;
}
.top{
	background-color: #515869;
	height: 8%;
	width: 100%;
	margin: 0;
	float: left;
}
.logo{
	position: relative;
	margin-left: 2%;
	height: 100%;
	width: 30%;
	float: left;
}
.info{
	
	position: relative;
	height: 100%;
	width: 30%;
	float: right;
}
.form-control{
	position: absolute;
	height: 50%;
	width: 30%;
	top: 25%;
	border-width: 0;
	border-radius:30px;
}
.manage{
	position: absolute;
	height: 70%;
	width:10%;
	top: 15%;
	left: 40%;
	float: left;
	border-radius: 20%;
}
.refresh{
	position: absolute;
	height: 50%;
	width:7%;
	top: 25%;
	left: 60%;
	float: left;
	border-radius: 50%;
}
.switch{
	position: absolute;
	height: 30%;
	width:10%;
	top: 35%;
	left: 80%;
	float: left;
}
.buttom{
	background-color: #706B6B;
	height: 92%;
	width: 100%;
	margin: 0;
	
}
.left{
	background-color: #515869;
	height: 100%;
	width: 13%;
	margin: 0;
	padding: 0;
	border: 0;
	float: left;

}
.right{
	background-color: #272821;
	height: 100%;
	width: 87%;
	margin: 0;
	padding: 0;
	border: 0;
	float: left;
}

/*左边导航栏*/
.data_cente,.news_manage,.inform_manage,.user_manage,.cpccs_work{
	position: relative;
	margin-left: 3%;
	margin-right: 0;
	margin-bottom: 5%;
	margin-top: 5%;
	height: 10%;
	font-size: 150%;
}
.div1{
	position: relative;
	float: left;
	height: 80%;
	width: 30%;
	margin-top: 5%;
	padding: 0;
	border: 0;
}
.div2{
	position: relative;
	float: left;
	width: 70%;
	margin-top: 17%;
	padding: 0;
	border: 0;
	color: #FFFFFF;
	font-family: STXinwei;
}