package com.wechat_small_program.entity;

/**
 * 对错题记录查询结果数据的封装
 * @author SZW
 * @date 2018年6月3日
 * @version 1.0 
 */
public class ErrorRecordPo {
	private String question;
	private String realAnswer;
	private String erroAnswer;
	private String A;
	private String B;
	private String C;
	private String D;
	
	public ErrorRecordPo() {
		super();
		// TODO Auto-generated constructor stub
	}

	public ErrorRecordPo(String question, String realAnswer,
			String erroAnswer, String a, String b, String c, String d) {
		super();
		this.question = question;
		this.realAnswer = realAnswer;
		this.erroAnswer = erroAnswer;
		A = a;
		B = b;
		C = c;
		D = d;
	}

	@Override
	public String toString() {
		return "ErrorRecordActionPo [question=" + question + ", realAnswer="
				+ realAnswer + ", erroAnswer=" + erroAnswer + ", A=" + A
				+ ", B=" + B + ", C=" + C + ", D=" + D + "]";
	}

	public String getQuestion() {
		return question;
	}

	public void setQuestion(String question) {
		this.question = question;
	}

	public String getRealAnswer() {
		return realAnswer;
	}

	public void setRealAnswer(String realAnswer) {
		this.realAnswer = realAnswer;
	}

	public String getErroAnswer() {
		return erroAnswer;
	}

	public void setErroAnswer(String erroAnswer) {
		this.erroAnswer = erroAnswer;
	}

	public String getA() {
		return A;
	}

	public void setA(String a) {
		A = a;
	}

	public String getB() {
		return B;
	}

	public void setB(String b) {
		B = b;
	}

	public String getC() {
		return C;
	}

	public void setC(String c) {
		C = c;
	}

	public String getD() {
		return D;
	}

	public void setD(String d) {
		D = d;
	}
	
}
