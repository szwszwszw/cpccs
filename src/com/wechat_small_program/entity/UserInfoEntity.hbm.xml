<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wechat_small_program.entity.UserInfoEntity" table="t_userInfo">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="uid" column="uid"></property>
		<property name="name" column="name"></property>
		<property name="sex" column="sex"></property>
		<property name="tel" column="tel"></property>
		<property name="college" column="college"></property>
		<property name="birthday" column="birthday"></property>
		<property name="hometown" column="hometown"></property>
		<property name="email" column="email"></property>
		<property name="qq" column="qq"></property>
	</class>
</hibernate-mapping>