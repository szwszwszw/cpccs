package com.wechat_small_program.entity;

/**
 * 对应数据库中的“题库（t_question_bank）”表，用于hibernate中ORM映射
 * @author SZW
 * @date 2018年5月22日
 * @version 1.0 
 */
public class QuestionBankEntity {
	private int id;
	private String question;
	private String answer;
	private String A;
	private String B;
	private String C;
	private String D;
	private String analysis;
	public QuestionBankEntity() {
		super();
		// TODO Auto-generated constructor stub
	}
	
	public QuestionBankEntity(int id, String question, String answer, String a,
			String b, String c, String d, String analysis) {
		super();
		this.id = id;
		this.question = question;
		this.answer = answer;
		A = a;
		B = b;
		C = c;
		D = d;
		this.analysis = analysis;
	}

	@Override
	public String toString() {
		return "QuestionBankEntity [id=" + id + ", question=" + question
				+ ", answer=" + answer + ", A=" + A + ", B=" + B + ", C=" + C
				+ ", D=" + D + ", analysis=" + analysis + "]";
	}

	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getQuestion() {
		return question;
	}
	public void setQuestion(String question) {
		this.question = question;
	}
	public String getAnswer() {
		return answer;
	}
	public void setAnswer(String answer) {
		this.answer = answer;
	}
	public String getA() {
		return A;
	}
	public void setA(String a) {
		A = a;
	}
	public String getB() {
		return B;
	}
	public void setB(String b) {
		B = b;
	}
	public String getC() {
		return C;
	}
	public void setC(String c) {
		C = c;
	}
	public String getD() {
		return D;
	}
	public void setD(String d) {
		D = d;
	}

	public String getAnalysis() {
		return analysis;
	}

	public void setAnalysis(String analysis) {
		this.analysis = analysis;
	}
	
}
