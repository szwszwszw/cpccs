package com.wechat_small_program.entity;

import java.util.Date;
/**
 * 对应数据库中的“用户信息（t_userInfo）”表，用于hibernate中ORM映射
 * @author SZW
 * @date 2018年5月22日
 * @version 1.0 
 */
public class UserInfoEntity {
	private int id;
	private String uid;
	private String name;
	private String sex;
	private String tel;
	private String college;
	private Date birthday;
	private String hometown;
	private String email;
	private String qq;
	
	public UserInfoEntity() {
		super();
		// TODO Auto-generated constructor stub
	}

	public UserInfoEntity(int id, String uid, String name, String sex,
			String tel, String college, Date birthday, String hometown,
			String email, String qq) {
		super();
		this.id = id;
		this.uid = uid;
		this.name = name;
		this.sex = sex;
		this.tel = tel;
		this.college = college;
		this.birthday = birthday;
		this.hometown = hometown;
		this.email = email;
		this.qq = qq;
	}

	@Override
	public String toString() {
		return "UserInfoEntity [id=" + id + ", uid=" + uid + ", name=" + name
				+ ", sex=" + sex + ", tel=" + tel + ", college=" + college
				+ ", birthday=" + birthday + ", hometown=" + hometown
				+ ", email=" + email + ", qq=" + qq + "]";
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getUid() {
		return uid;
	}

	public void setUid(String uid) {
		this.uid = uid;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getSex() {
		return sex;
	}

	public void setSex(String sex) {
		this.sex = sex;
	}

	public String getTel() {
		return tel;
	}

	public void setTel(String tel) {
		this.tel = tel;
	}

	public String getCollege() {
		return college;
	}

	public void setCollege(String college) {
		this.college = college;
	}

	public Date getBirthday() {
		return birthday;
	}

	public void setBirthday(Date birthday) {
		this.birthday = birthday;
	}

	public String getHometown() {
		return hometown;
	}

	public void setHometown(String hometown) {
		this.hometown = hometown;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getQq() {
		return qq;
	}

	public void setQq(String qq) {
		this.qq = qq;
	}
	
}
