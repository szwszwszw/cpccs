<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.wechat_small_program.entity.ErrorEntity" table="t_error_list">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="uid" column="uid"></property>
		<property name="error_id" column="error_id"></property>
		<property name="erroChoose" column="erroChoose"></property>
	</class>
</hibernate-mapping>